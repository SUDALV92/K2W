switch(mode)
{
 case 1: //intro start
  switch(direction)
  {
   case 0:    
    if(x &gt;= targetX)
     hspeed = 0;
    else
     hspeed = lerp(16,3,x/targetX);
    break;
   case 90:
    if(y &lt;= targetY)
     vspeed = 0;
    else
     vspeed = -lerp(10,3,targetY/y);    
    break;
   case 180:
    if(x &lt;= targetX)
     hspeed = 0;
    else
     hspeed = -lerp(16,3,targetX/x);
    break;
   case 270:
    if(y &gt;= targetY)
     vspeed = 0;
    else
     vspeed = lerp(10,3,y/targetY);
    break;
  }
  break;
 case 2:
  if(point_distance(x,y,targetX,targetY) &lt; 300)
  {
   friction = random_range(0.05,0.15);
  }
  break;
 case 5:
  if(x &lt;= 40 || x &gt;= 760 || y &lt; 40)
  {
   repeat(2)
   {
    apple = instance_create(x,y,objExtraStageBloodFestivalApple);
    apple.sprite_index = scrRandomAppleColor();
    apple.image_xscale = 0.8;
    apple.image_yscale = 0.8;
    if(x &lt;= 400)
     apple.direction = random_range(300,400);
    else
     apple.direction = random_range(140,210);
    
    apple.speed = random_range(2,10);
    apple.gravity = 0.1;
   }
   instance_destroy();
  }  
  break;
 case 6:
  if(y &lt;= 40 || y &gt;= 568)
  {
   repeat(3)
   {
    apple = instance_create(x,y,objExtraStageBloodFestivalApple);
    apple.sprite_index = scrRandomAppleColor();
    apple.image_xscale = 0.8;
    apple.image_yscale = 0.8;
    if(y &lt;= 40)
    {
     apple.direction = random_range(180,360);
     apple.speed = random_range(5,10);
    }
    else
    {
     apple.direction = random_range(40,140);     
     apple.speed = random_range(10,15);
    }   
    
    apple.gravity = 0.1;
   }
   instance_destroy();
  }  
  break;
 case 8:
  if(x &lt; 0) { hspeed = abs(hspeed); if(state == 1) { gravity = 0; friction = 0.1; }}
  if(x &gt; 800) { hspeed = -abs(hspeed); if(state == 1) { gravity = 0; friction = 0.1; }}
  if(y &lt; 0) { vspeed = abs(vspeed); if(state == 1) { gravity = 0; friction = 0.1; }}
  if(y &gt; 608) { vspeed = -abs(vspeed); if(state == 1) { gravity = 0; friction = 0.1; }}
  outsideDelete = false;
  break;
 case 9:
  if(x &lt; 0) { hspeed = abs(hspeed); }
  if(x &gt; 800) { hspeed = -abs(hspeed); }
  if(y &lt;= 8) { vspeed = 0 }
  if(y &gt; 590) { vspeed = 0 }
  break;
 case 10:
  x += xspeed;
  break;
 case 11:
    i = (i + 10) mod 360;
    shift = 40 * dsin(i);
    
    xx += hspeed;
    yy += vspeed;
    
    x = xx + lengthdir_x(shift,direction + 90*state);
    y = yy + lengthdir_y(shift,direction + 90*state);
    break;
 case 12:
      if(image_alpha &lt; 1)
      {
       image_alpha += 0.01;
      }
      x = 400 + lengthdir_x(len,dir);
      y = 304 + lengthdir_y(len,dir);
      
      if(len &lt; -60)
       maxLen = 60;
       
      if(state == 0)
      {
       if(len &gt; -maxLen)
       {
        if(maxLen &gt; 60)
         len -= 8
        else
        len -= 4;
       }
       else
       {
        state = 1;
        depth = choose(0,-1,-2,-3);
       }
      }
      else if(state == 1)
      {
       if(len &lt; maxLen)
       {
        len += 4;
       }
       else
       {
        state = 0;
        depth = choose(0,-1,-2,-3);
       }
      }
      if(len == 0 &amp;&amp; finalSpawn)
      {
       repeat(12)
       {
        apple = instance_create(irandom_range(150,650),irandom_range(150,458),objExtraStageBloodFestivalApple);
        apple.sprite_index = scrRandomAppleColor();
        apple.image_alpha = 0.5;
        apple.image_xscale = 0.8;
        apple.image_yscale = 0.8;
        apple.direction = point_direction(400,304,apple.x,apple.y);
        apple.mode = 13;
        apple.depth = 1;
        apple.alarm[2] = 50;
       }
      }
      break;
 case 13:
      if(distance_to_point(400,304) &lt;= 1)
      {
       instance_destroy();
      }
      image_alpha += 0.01;
      break;
}
