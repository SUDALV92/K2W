     if(instance_exists(objChaostt3)) { orbsCount++; sumHP += objChaostt3.hp; }
     if(instance_exists(objChaostt4)) { orbsCount++; sumHP += objChaostt4.hp; }
     if(instance_exists(objChaostt5)) { orbsCount++; sumHP += objChaostt5.hp; }
