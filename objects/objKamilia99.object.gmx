            flame = instance_create(x,ystart-400,objMarioFlame);
            flame.vspeed = 10;
        }
    }
    if(attackType[8])
    {
        if(i == 50 || i == 80 || i == 110|| i == 140 ||
           i == 250 || i == 280 || i == 310 || i == 340)
        {
            hadoukenX = irandom(36);
            repeat(60)
            {
                if(hadoukenShifted)
                    hadouken = instance_create(hadoukenX+36,-80,objHadoken);
                else
                    hadouken = instance_create(hadoukenX,-80,objHadoken);   
