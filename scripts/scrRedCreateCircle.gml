/// scrRedCreateCircle( spawnX, spawnY, angle, bulletCount, bulletObj )
///spawns a ring of projectiles
///argument0 - spawn X
///argument1 - spawn Y
///argument2 - starting angle
///argument3 - number of projectiles to spawn
///argument4 - projectile to spawn

var circle = scrRedCreateCircleAdditionalParams();
scrRedInitializeCircle( argument0, argument1, argument2, argument3, argument4, circle );
return circle;
